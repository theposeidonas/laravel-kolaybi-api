{
  "name": "theposeidonas/laravel-kolaybi-api",
  "description": "KolayBi API for Laravel Projects",
  "keywords": [
    "laravel",
    "kolaybi",
    "invoice",
    "billing"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Baran Arda",
      "email": "baran@webremium.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/validation": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Theposeidonas\\LaravelKolaybiApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Theposeidonas\\LaravelKolaybiApi\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Theposeidonas\\LaravelKolaybiApi\\KolaybiServiceProvider"
      ],
      "aliases": {
        "Kolaybi": "Theposeidonas\\LaravelKolaybiApi\\Facades\\Kolaybi"
      }
    }
  },
  "prefer-stable": true
}